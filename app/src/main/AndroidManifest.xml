<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jixuan.ningningapp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/donkey"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="jixuan.ningningapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="jixuan.ningningapp.SecondActivity">
            <intent-filter>
                <action android:name="jixuan.ningningapp.My_ACTION"/>
                <action android:name="android.intent.action.DEFAULT" />
                <category android:name="android.intent.category.default"/>
            </intent-filter>
        </activity>

        <activity android:name="jixuan.ningningapp.ThirdActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="http"/>
            </intent-filter>
        </activity>


    </application>

</manifest>
